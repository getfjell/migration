{
    "name": "@fjell/migration",
    "version": "4.3.0",
    "license": "UNLICENSED",
    "description": "Migration Library for Fjell",
    "engines": {
        "node": ">=21"
    },
    "main": "dist/src/index.js",
    "exports": {
        ".": {
            "require": "./dist/src/index.js",
            "import": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "build": "yarn run lint && tsc && tsc-alias -p tsconfig.json",
        "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
        "lint": "eslint .",
        "clean": "rimraf dist",
        "test": "yarn run lint && NODE_OPTIONS='--experimental-require-module' yarn run jest --coverage"
    },
    "dependencies": {
        "@fjell/core": "^4.3.1",
        "@fjell/lib": "^4.3.0",
        "@fjell/logging": "^4.3.4",
        "@google-cloud/firestore": "^7.5.0",
        "@google-cloud/storage": "^7.9.0",
        "dayjs": "^1.11.13",
        "deepmerge": "^4.3.1",
        "multer": "^1.4.5-lts.1",
        "sequelize": "^6.37.5",
        "specifier-resolution-node": "^1.1.4",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-typescript": "^7.24.7",
        "@tsconfig/recommended": "^1.0.8",
        "@types/babel__core": "^7",
        "@types/babel__preset-env": "^7",
        "@types/jest": "^29.5.14",
        "@types/luxon": "^3",
        "@typescript-eslint/eslint-plugin": "^7",
        "@typescript-eslint/parser": "^7",
        "babel-jest": "^29.7.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.7.3"
    },
    "packageManager": "yarn@4.6.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/getfjell/migration.git"
    }
}
